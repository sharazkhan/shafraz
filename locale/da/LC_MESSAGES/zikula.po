msgstr "george,mindresikkert,george@example.org,1,0,1|5"

#: system/Users/templates/users_admin_import.tpl:61
msgid "robert,hispassword,robert@example.org,,,"
msgstr "robert,hansadgangskode,robert@example.org,,,"

#: system/Users/templates/users_admin_import.tpl:64
msgid "Another example of a valid CSV file"
msgstr "Et andet eksempel på en gyldig CSV fil"

#: system/Users/templates/users_admin_import.tpl:66
msgid "albert,12secure09,albert@example.org"
msgstr "albert,12sikkert09,albert@example.org"

