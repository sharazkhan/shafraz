<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>Z Velo</title>
{pageaddvar name="stylesheet" value="$stylepath/styles.css"}
</head>

<body>
<div class="containter">
	
    <div class="inner">
    	  <div class="header">
                 <div class="logo">
                 ZV<span class="char_space_sync">elo</span>
                 <span class="logo-version">version 1.0.0</span>
                 </div>
                 
                    
          </div>   
          <div class="content_sec">
          	<div class="content_left">
            	 {blockposition name='zvelo-left'}
               </div>
            <div class="content_middle">
            	<div class="address_sec">
                	<div class="addr asec1">
                    	<div class="text_sep1">Clientno<br /><input type="text" class="grey_box w59" />
                        </div>
                        <div class="text_sep2">Created<br /><input type="text" class="grey_box w104" />

                        </div>
                        
                    </div>
                    
                    <div class="addr asec2">First name<br />
                    <input type="text" class="orange_box full_width1" />
                    </div>
                     <div class="addr asec2">Last name<br />
                    <input type="text" class="orange_box full_width1" />
                    </div>
                    
                     <div class="addr asec2 ht58">Address<br />
                    <input type="text" class="orange_box full_width1" /><br />
                     <input type="text" class="orange_box full_width1 slight_top" /><br />
                    </div>
                   <div class="addr asec1">
                    	<div class="text_sep1">ZIP<br /><input type="text" class="orange_box w59" /> </div>
                        <div class="text_sep2">City<br /><input type="text" class="orange_box w104" /></div>                        
                    </div>
                     <div class="addr asec2">Phone<br />
                    <input type="text" class="orange_box full_width1" />
                    </div>
                     <div class="addr asec2">Email<br />
                    <input type="text" class="orange_box full_width1" />
                    </div>
                     <div class="addr asec2 ht100">Comments<br />
                     <textarea class="orange_box full_width1 ht90" rows="5" >
                     </textarea>
                   
                    </div>
                	
                </div>
                
                <div class="address_image_sec">
                	
                    <div class="tophr_sec">
                    <hr class="tophr"/>                    	
                    </div>
                    <div class="tophr_sec2">
                    <hr class="tophr2"/>
                    	
                    </div>
                    <div class="tophr_sec3 right">                     	
                        <div>
                        	<div class="top_box"></div>
                            <div class="chest_row">  <hr class="tophr3"/> </div>                            
                     	</div>
                     </div>
                     
                    <div class="tophr_sec4"> 
                     		<div class="bot_box"></div>              	
                    </div>
                    
                     <div class="tophr_sec5"> 
                        <div class="margin40"><hr class="tophr3"/></div>
                     	<div><hr class="tophr3"/></div>
                    </div>
                    
                    <div class="tophr_sec3 right">                     	
                        <div>
                        	<div class="top_box"></div>
                                                    
                     	</div>
                     </div>
                     
                    <div class="tophr_sec4"> 
                     		<div class="bot_box"></div>              	
                    </div>
                    
                     <div class="tophr_sec5"> 
                        <div class="margin30"><hr class="tophr3"/></div>
                     
                    </div>
                    	
                </div>
               
                
                <div class="address_label_sec">
                
                	<p class="LabelMap l_top1">Gender</p>
                   	<p class="LabelMap l_top2">Functional height</p>
                    <p class="LabelMap l_top3">Shoulder height</p>
                    <p class="LabelMap l_top4">Shoulder width</p>
                    <p class="LabelMap l_top5">Arm length</p>
                    <p class="LabelMap l_top6">Pelvic bone height</p>
                    <p class="LabelMap l_top7">Fist height</p>
                    <p class="LabelMap l_top8">Leg length</p>
                    <p class="LabelMap l_top9">Weight</p>
                  
                	
                </div>
                <div class="address_textbox_sec">
                	<p class="LabelMap l_top1_tx"><input type="text" class="grey_box w61" />cm</p>
                   	<p class="LabelMap l_top2_tx"><input type="text" class="grey_box w61" />cm</p>
                    <p class="LabelMap l_top3_tx"><input type="text" class="grey_box w61" />cm</p>
                    <p class="LabelMap l_top4_tx"><input type="text" class="grey_box w61" />cm</p>
                    <p class="LabelMap l_top5_tx"><input type="text" class="grey_box w61" />cm</p>
                    <p class="LabelMap l_top6_tx"><input type="text" class="grey_box w61" />cm</p>
                    <p class="LabelMap l_top7_tx"><input type="text" class="grey_box w61" />cm</p>
                    <p class="LabelMap l_top8_tx"><input type="text" class="grey_box w61" />cm</p>
                    <p class="LabelMap l_top9_tx"><input type="text" class="grey_box w61" />cm</p>
                  
                	
                </div>
            
              
            </div>
            
            <div class="content_right">
            	 {blockposition name='zvelo-right'}
              
            </div>
          	
          </div>
    </div>

</div>
</body>
</html>
